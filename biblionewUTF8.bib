@Article{Henon2003,
  Title                    = {New families of periodic orbits in {H}ill's problem of three bodies},
  Author                   = {H{\'e}non, M.},
  Year                     = {2003},
  Language                 = {english},
  Pages                    = {223--246},
  Volume                   = {85},

  Abstract                 = {The search for symmetric periodic solutions of Hill's problem, begun in H\'enon (1996) for simple-periodic orbits, is extended to double- and triple-periodic orbits. Seven new families are found.We also complete the description of family g3, begun in H\'enon (1970). In all of these families, both ends consist of orbits of increasing dimensions with $\Gamma \rightarrow \infty$. They can be continued into second species families, which are identified.},
