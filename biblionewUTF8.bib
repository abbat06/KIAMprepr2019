}

@Article{Henon2005,
  Title                    = {Families of asymmetric periodic orbits in {H}ill's problem of three bodies},
  Author                   = {H{\'e}non, Michel},
  Year                     = {2005},
  Language                 = {english},
  Pages                    = {87-100},
  Volume                   = {93},

  Abstract                 = {We present five families of periodic solutions of Hill's problem
 which are asymmetric with respect to the horizontal $\xi$ axis.
 In one of these families, the orbits are symmetric with respect to the vertical $\eta$
 axis; in the four others, the orbits are without any symmetry.
 Each family consists of two branches, which are mirror images of each other with
 respect to the $\xi$ axis. These two branches are joined at a maximum of $\Gamma$,
 where the family of asymmetric periodic solutions intersects a family of symmetric
 (with respect to the $\xi$ axis) periodic solutions.
 Both branches can be continued into second species families for $\Gamma\rightarrow-\infty$.
},
  Date-modified            = {2014-01-21 15:16:21 +0000},
  Journal                  = {Celest. Mech. Dyn. Astr.},
  Keywords                 = {Hill's problem, numerical integration, periodic orbits, second species families}
}

